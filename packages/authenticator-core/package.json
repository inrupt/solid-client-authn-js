{
  "name": "@solid/authenticator-core",
  "version": "0.0.0",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "build-watch": "tsc-watch --preserveWatchOutput -p tsconfig.build.json",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "lint": "tslint -t stylish --project \"tsconfig.json\""
  },
  "devDependencies": {
    "tsc-watch": "^4.0.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.0.1"
  }
}
